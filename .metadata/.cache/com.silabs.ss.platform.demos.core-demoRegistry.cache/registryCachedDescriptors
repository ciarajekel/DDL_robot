    <properties key="core.boardCompatibility" value="brd3800a"/>
    <properties key="demos.imageFile" value="file:/C:/SiliconLabs/SimplicityStudio/v4/developer/sdks/gecko_sdk_suite/v2.5/app/mcu_example/EFM32WG_DK3850/lines/bin/lines.bin"/>
    <properties key="coreSdk.contributingSdk" value="com.silabs.sdk.stack.super:2.5.0._310455071"/>
    <properties key="core.detected" value="true"/>
